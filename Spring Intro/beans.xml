<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-init-method="onCreate" default-destroy-method="onDestroy">


	<bean id="osoba" class="rs.ac.singidunum.fir.Osoba">
		<constructor-arg name="ime" type="String"
			value="Pera Peric">
		</constructor-arg>
		<constructor-arg name="jmbg" type="int" value="12345667"></constructor-arg>
		<property name="uzrast" value="35"></property>
		<property name="adresa" ref="adresa"></property>
	</bean>
	<bean id="adresa" class="rs.ac.singidunum.fir.Adresa">
		<constructor-arg name="ulica" type="String"
			value="Danijelova 32">
		</constructor-arg>
		<constructor-arg name="postanskiBroj" type="String"
			value="11000">
		</constructor-arg>
	</bean>
</beans>
